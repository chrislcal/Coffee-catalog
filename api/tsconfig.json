{
    "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "target": "ES2020",
      "sourceMap": true,
      "outDir": "./dist",
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"],
    "types": ["node", "jest", "supertest"]
  }
  